		<row>
			<field>
				<name>nombre</name>
				<type>label</type>
				<class>tbody</class>
			</field>
			<field>
				<name>usuario</name>
				<type>label</type>
				<class>tbody</class>
			</field>
			<field>
				<name>grupo</name>
