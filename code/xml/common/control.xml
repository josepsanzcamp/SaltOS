<root>
	<fieldset ifeval="!ismobile()">
		<width>700px</width>
		<title lang="true">registerdata</title>
		<icon eval="true">ICON("files")</icon>
		<popup>true</popup>
		<head>
			<field>
				<type>label</type>
				<label lang="true">nombre</label>
				<class>thead</class>
			</field>
			<field>
