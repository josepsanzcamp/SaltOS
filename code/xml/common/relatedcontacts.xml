<root>
	<contactos>
		<fieldset>
			<show eval="true">check_user("contactos","menu")?"true":"false"</show>
			<width>700px</width>
			<title lang="true">contactdata</title>
			<head>
				<field>
					<type>label</type>
					<label lang="true">nombre</label>
					<class>thead</class>
				</field>
				<field>
					<type>label</type>
					<label lang="true">tel_fijo</label>
					<class>thead</class>
				</field>
				<field>
					<type>label</type>
					<label lang="true">tel_movil</label>
					<class>thead</class>
				</field>
				<field>
					<type>label</type>
					<label lang="true">email</label>
					<class>thead</class>
				</field>
				<field>
					<type>separator</type>
					<class>thead</class>
					<colspan>100</colspan>
				</field>
			</head>
			<row>
				<field>
					<type>label</type>
					<name>nombre</name>
					<class>tbody</class>
				</field>
				<field>
					<type>label</type>
					<name>tel_fijo</name>
					<class>tbody</class>
				</field>
				<field>
					<type>label</type>
					<name>tel_movil</name>
					<class>tbody</class>
				</field>
				<field>
					<type>label</type>
					<name>email</name>
					<class>tbody</class>
				</field>
				<field>
					<name>link_view</name>
					<type>link</type>
					<label lang="true">view</label>
					<width>1px</width>
					<icon>view</icon>
					<class>tbody</class>
				</field>
				<field>
					<name>link_vcard</name>
					<type>link</type>
					<label lang="true">vcard</label>
					<width>1px</width>
					<icon>vcard</icon>
					<class>tbody</class>
				</field>
				<field>
					<name>link_qrcode</name>
					<type>link</type>
					<label lang="true">qrcode</label>
					<width>1px</width>
					<icon>qrcode</icon>
					<class>tbody</class>
				</field>
			</row>
		</fieldset>
	</contactos>
</root>
