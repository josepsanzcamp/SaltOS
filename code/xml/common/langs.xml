<root>
	<name>lang</name>
	<label lang="true">language</label>
	<type>select</type>
	<width>120px</width>
	<rows>
		<row>
			<label>Arabic (Saudi Arabia)</label>
			<value>ar_SA</value>
		</row>
		<row>
			<label>Català (Catalunya)</label>
			<value>ca_CT</value>
		</row>
		<row>
			<label>Deutsch (Deutschland)</label>
			<value>de_DE</value>
		</row>
		<row>
			<label>English (USA)</label>
			<value>en_US</value>
		</row>
		<row>
			<label>Español (España)</label>
			<value>es_ES</value>
		</row>
		<row>
			<label>Español (América Latina)</label>
			<value>es_LA</value>
		</row>
		<row>
			<label>French (France)</label>
			<value>fr_FR</value>
		</row>
		<row>
			<label>Português (Brasil)</label>
			<value>pt_BR</value>
		</row>
		<row>
			<label>Português (Portugal)</label>
			<value>pt_PT</value>
		</row>
	</rows>
	<value global="lang" eval="true">$lang</value>
	<onchange>submit1()</onchange>
</root>
