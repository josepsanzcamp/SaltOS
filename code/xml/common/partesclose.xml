<root>
    <partesclose>
        <fieldset ifeval="check_user('partes','menu')">
            <width>700px</width>
            <title lang="true">partesclose</title>
            <icon eval="true">ICON("form")</icon>
            <head>
                <field>
                    <type>label</type>
                    <label lang="true">id</label>
                    <class>thead</class>
                </field>
                <field>
                    <type>label</type>
                    <label lang="true">username</label>
                    <class>thead</class>
                </field>
                <field>
                    <type>label</type>
                    <label lang="true">date</label>
                    <class>thead</class>
                </field>
                <field>
                    <type>label</type>
                    <label lang="true">tarea</label>
                    <class>thead</class>
                </field>
                <field>
                    <type>label</type>
                    <label lang="true">horas</label>
                    <class>thead</class>
                    <math>
                        <func>sum()</func>
                        <label lang="true">suma</label>
                    </math>
                </field>
                <field>
                    <type>label</type>
                    <label eval="true">LANG("precio")." (".CONFIG("accounting_currency").")"</label>
                    <class>thead</class>
                    <math>
                        <func>avg()</func>
                        <label eval="true">LANG("average")." (".CONFIG("accounting_currency").")"</label>
                    </math>
                </field>
                <field>
                    <type>label</type>
                    <label eval="true">LANG("total")." (".CONFIG("accounting_currency").")"</label>
                    <class>thead</class>
                    <math>
                        <func>sum()</func>
                        <label eval="true">LANG("suma")." (".CONFIG("accounting_currency").")"</label>
                    </math>
                </field>
                <field>
                    <type>label</type>
                    <label lang="true">liquidado</label>
                    <class>thead</class>
                </field>
                <field>
                    <type>separator</type>
                    <class>thead</class>
                    <colspan>100</colspan>
                    <math>
                        <ignore>true</ignore>
                    </math>
                </field>
            </head>
            <row>
                <field>
                    <type>label</type>
                    <name>id</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>usuario</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>fecha</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>tarea</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>horas</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>precio</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>total</name>
                    <class>tbody</class>
                </field>
                <field>
                    <type>label</type>
                    <name>liquidado</name>
                    <class>tbody</class>
                </field>
                <field>
                    <name>link_view</name>
                    <type>link</type>
                    <label lang="true">view</label>
                    <width>1px</width>
                    <icon eval="true">ICON("view")</icon>
                    <class>tbody</class>
                </field>
                <field>
                    <name>link_pdf</name>
                    <type>link</type>
                    <label lang="true">pdf</label>
                    <width>1px</width>
                    <icon eval="true">ICON("pdf")</icon>
                    <class>tbody</class>
                </field>
                <field>
                    <name>link_view2</name>
                    <type>link</type>
                    <label lang="true">view2</label>
                    <width>1px</width>
                    <icon eval="true">ICON("viewpdf")</icon>
                    <class>tbody</class>
                </field>
                <field>
                    <name>link_mail</name>
                    <type>link</type>
                    <label lang="true">mail</label>
                    <width>1px</width>
                    <icon eval="true">ICON("correo")</icon>
                    <class>tbody</class>
                </field>
            </row>
        </fieldset>
    </partesclose>
</root>
