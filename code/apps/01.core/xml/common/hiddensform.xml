<root>
    <field>
        <name>page</name>
        <type>hidden</type>
        <value global="page" eval="true">$page</value>
    </field>
    <field>
        <name>action</name>
        <type>hidden</type>
        <value global="action" eval="true">$action</value>
    </field>
    <field>
        <name>id</name>
        <type>hidden</type>
        <value global="id" eval="true">$id</value>
    </field>
    <field>
        <name>returnhere</name>
        <type>hidden</type>
        <value eval="true">getParam("returnhere")</value>
    </field>
    <field>
        <name>returnback</name>
        <type>hidden</type>
        <value eval="true">getParam("returnback")</value>
    </field>
    <field>
        <name>is_fichero</name>
        <type>hidden</type>
        <value global="is_fichero" eval="true">$is_fichero=intval(getParam("is_fichero"))</value>
    </field>
    <field>
        <name>is_buscador</name>
        <type>hidden</type>
        <value global="is_buscador" eval="true">$is_buscador=intval(getParam("is_buscador"))</value>
    </field>
</root>
